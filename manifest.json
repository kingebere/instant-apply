{
	"name": "Instant Apply",
	"description": "Instant Apply",
	"version": "1.0",
	"manifest_version": 3,
	"background": {
		"service_worker": "supabase.js"
	},
	"action": {
		"default_title": "Instant Apply",
		"default_popup": "background.html"
	},
	"permissions": ["scripting", "activeTab", "tabs", "storage"],
	"content_scripts": [
		{
			"matches": ["https://*.boards.greenhouse.io/*"],
			"js": ["instant.js"]
		},
		{
			"matches": ["https://*.instantapply.co/*"],
			"js": ["index.js", "config.js", "accesstoken.js"]
		}
	]
}
